Sub DeleteRowsNotMatching(strEcrBusinessUnit As String)
    Application.ScreenUpdating = False
    
    Dim ws As Worksheet
    Set ws = ThisWorkbook.Sheets("OUTPUT") ' Update to your actual sheet name
    
    Dim LastRow As Long
    LastRow = ws.Cells(ws.Rows.Count, "CR").End(xlUp).Row
    
    Dim i As Long
    For i = LastRow To 1 Step -1
        If ws.Cells(i, "CR").Value <> strEcrBusinessUnit Then
            ws.Rows(i).Delete
        End If
    Next i
    
    Application.ScreenUpdating = True
    MsgBox "Rows deleted successfully!"
End Sub


Macro Name: DeleteRowsNotMatching
Parameter: {$strEcrBusinessUnit$}
