Option Explicit
Sub Sync()

    Dim wb As Workbook
    Dim copySheet As Worksheet
    Dim outputSheet As Worksheet
    Dim lastRowCopy As Long
    Dim dataRange As Range

    ' Activate any workbook that contains "20" in the filename
    For Each wb In Workbooks
        If InStr(wb.Name, "20") > 0 Then
            wb.Activate
            Exit For
        End If
    Next wb

    Set copySheet = Sheets("Copy")
    Set outputSheet = Sheets("Output")

    ' Find last row of data in Copy sheet from AF2 downward
    lastRowCopy = copySheet.Cells(copySheet.Rows.Count, "AF").End(xlUp).Row

    ' Define the data range from AF2 to AH(last)
    Set dataRange = copySheet.Range("AF2:AH" & lastRowCopy)

    ' Write the data to Output sheet starting from AF16
    outputSheet.Range("AF16").Resize(dataRange.Rows.Count, 3).Value = dataRange.Value

    ' Optional: reactivate macro file if needed
    Windows("Ajith Macro V1.xlsm").Activate

End Sub
