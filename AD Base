-- Extract number from $strHeaderFiveValue$
UPDATE YourTable
SET SomeColumn = 'SomeValue'
WHERE 
    (
        (
            -- Check for 'greater than' condition (e.g., >10 years)
            '$strHeaderFiveValue$' LIKE '>%'
            AND YEAR(CURRENT_DATE) - CAST(SUBSTR([Date Column Name], 7, 4) AS INTEGER) > CAST(SUBSTR('$strHeaderFiveValue$', 2, LENGTH('$strHeaderFiveValue$') - 6) AS INTEGER)
        )
        OR
        (
            -- Check for 'less than' condition (e.g., <20 years)
            '$strHeaderFiveValue$' LIKE '<%'
            AND YEAR(CURRENT_DATE) - CAST(SUBSTR([Date Column Name], 7, 4) AS INTEGER) < CAST(SUBSTR('$strHeaderFiveValue$', 2, LENGTH('$strHeaderFiveValue$') - 6) AS INTEGER)
        )
    )
