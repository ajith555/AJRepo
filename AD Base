UPDATE YourTable
SET SomeColumn = 'SomeValue'
WHERE 
    (
        -- Check if $strHeaderFiveValue$ has '>X years' condition
        (
            INSTR('$strHeaderFiveValue$', '>') > 0 AND
            YEAR(GETDATE()) - CAST(SUBSTRING([Latest hire date], 7, 4) AS INT) > CAST(SUBSTRING('$strHeaderFiveValue$', 2, LENGTH('$strHeaderFiveValue$') - 6) AS INT)
        )
        OR
        -- Check if $strHeaderFiveValue$ has '<X years' condition
        (
            INSTR('$strHeaderFiveValue$', '<') > 0 AND
            YEAR(GETDATE()) - CAST(SUBSTRING([Latest hire date], 7, 4) AS INT) < CAST(SUBSTRING('$strHeaderFiveValue$', 2, LENGTH('$strHeaderFiveValue$') - 6) AS INT)
        )
    )
