' Attempt to press Yes to All 3 times
    Call AutoClickYesToAll

End Sub

Sub AutoClickYesToAll()
    Dim i As Integer
    For i = 1 To 3
        ' Send Enter key (in some popups, Enter = Yes to All)
        Application.SendKeys "~", True
        ' Or use Alt+Y if that's the active shortcut
        Application.SendKeys "%Y", True

        ' Wait a bit before next keypress
        Application.Wait Now + TimeValue("00:00:01")
    Next i
End Sub
