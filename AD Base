UPDATE [Output$]
SET 
  F32 = (SELECT TOP 1 C.F32 FROM [Copy$] C WHERE C.F1 = [Output$].F1),
  F33 = (SELECT TOP 1 C.F33 FROM [Copy$] C WHERE C.F1 = [Output$].F1),
  F34 = (SELECT TOP 1 C.F34 FROM [Copy$] C WHERE C.F1 = [Output$].F1)
WHERE EXISTS (
  SELECT 1 FROM [Copy$] C WHERE C.F1 = [Output$].F1
)
