Sub Sync()

    Dim wb As Workbook

    ' Activate any workbook that contains "20" in the filename
    For Each wb In Workbooks
        If InStr(wb.Name, "20") > 0 Then
            wb.Activate
            Exit For
        End If
    Next wb

    ' Copy values from AF2:AH2 of "Copy" to same range of active sheet
    Dim copySheet As Worksheet
    Dim targetSheet As Worksheet

    Set copySheet = Sheets("Copy")
    Set targetSheet = ActiveSheet

    targetSheet.Range("AF2:AH2").Value = copySheet.Range("AF2:AH2").Value

End Sub
