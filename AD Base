    (
        ('$strHeaderFiveValue$' = 'All') OR 
        (
            CAST(SUBSTR([ReformattedDate], 7, 4) || SUBSTR([ReformattedDate], 4, 2) || SUBSTR([ReformattedDate], 1, 2) AS INTEGER)
            $op$
            CAST(SUBSTR('$cutoffDate$', 7, 4) || SUBSTR('$cutoffDate$', 4, 2) || SUBSTR('$cutoffDate$', 1, 2) AS INTEGER)
        )
    )
