# SharePoint Online site URL and folder path
$siteUrl = "https://yourtenant.sharepoint.com/sites/yoursite"
$folderPath = "/sites/yoursite/Shared Documents/YourFolder"

# Credentials (you will be prompted to enter your password)
$credentials = Get-Credential

# Connect to SharePoint Online
Connect-SPOService -Url $siteUrl -Credential $credentials

# Retrieve and list files
$files = Get-SPOFile -FolderUrl $folderPath
foreach ($file in $files) {
    Write-Output $file.Name
}

# Disconnect
Disconnect-SPOService
