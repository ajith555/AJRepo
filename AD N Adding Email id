# Define variables
$SiteURL = "https://ubscloud.sharepoint.com/teams/ABCDAUTOMATION"
$FolderRelativeURL = "/Move"

# Connect to SharePoint Online
Connect-PnPOnline -Url $SiteURL -Interactive

# Get the files in the specified folder
$Files = Get-PnPFolderItem -FolderSiteRelativeUrl $FolderRelativeURL -ItemType File

# Display the files
$Files | Select-Object Name, ServerRelativeUrl
