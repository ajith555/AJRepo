import requests

def make_authenticated_request(url, email, password):
    try:
        # Create a session with the provided credentials
        session = requests.Session()
        session.auth = (email, password)

        # Send a GET request to the website
        response = session.get(url)
        response.raise_for_status()  # Raises HTTPError for bad responses

        # If successful, print the content or handle it as needed
        print(response.text)

    except requests.exceptions.RequestException as e:
        print(f"Error: {e}")

# Replace 'your_website_url', 'your_email', and 'your_password'
make_authenticated_request('your_website_url', 'your_email@example.com', 'your_password')
