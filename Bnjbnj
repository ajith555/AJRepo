$url = "https://ajith.com"
$response = Invoke-WebRequest -Uri $url
$html = $response.Content

# Define regex pattern to extract URLs
$pattern = 'https:\/\/ajith\.com\/[\w-]+\/exports\/download'

# Extract URLs using regex
$urls = [regex]::Matches($html, $pattern) | ForEach-Object { $_.Value }

# Output the URLs
foreach ($url in $urls) {
    Write-Output $url
}
