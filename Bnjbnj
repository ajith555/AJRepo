# Define the base URL and the endpoint
$baseUrl = "https://ubs.invisionapp.com/overview/"
$endpoint = "/exports/initiate/pdf?method=overviewMenu&sortBy=1&sortOrder=1&ViewLayout=2"

# Loop through all the files (assuming 100 files)
for ($i = 1; $i -le 100; $i++) {
    # Construct the URL
    $url = $baseUrl + "file$i" + $endpoint

    # Output the URL
    Write-Output $url
}
