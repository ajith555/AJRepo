W
=IF(COUNTIFS($A$3:A3, A3, $E$3:E3, E3)=1, "First", "Duplicate")



X
=IF(W3="First", TEXTJOIN(", ", TRUE, IF(($A$3:$A$1000=A3)*($E$3:$E$1000=E3)*ISNUMBER(MATCH(TRUE, ISNUMBER(SEARCH({"STOP","HIT","SUSP","CLOSED","100K","OVERLIMIT"}, $E$3:$E$1000)), 0)), $B$3:$B$1000, "")), "")


Y
=IF(W3="First", TEXTJOIN(", ", TRUE, IF(($A$3:$A$1000=A3)*($E$3:$E$1000=E3)*ISNUMBER(MATCH(TRUE, ISNUMBER(SEARCH({"STOP","HIT","SUSP","CLOSED","100K","OVERLIMIT"}, $E$3:$E$1000)), 0)), $B$3:$B$1000, "")), "")

