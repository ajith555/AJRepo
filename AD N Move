W
=IF(COUNTIF($A$3:A3, A3)=1, "First", "Duplicate")

X
=IF(W3="First", TEXTJOIN(", ", TRUE, IF(($A$3:$A$1000=A3)*(COUNTIFS($A$3:$A$1000, A3, $E$3:$E$1000, "*"&$E$3:$E$1000&"*")>1), $B$3:$B$1000, "")), $B3)


Y
=IF(W3="First", TEXTJOIN(", ", TRUE, IF(($A$3:$A$1000=A3)*(COUNTIFS($A$3:$A$1000, A3, $E$3:$E$1000, "*"&$E$3:$E$1000&"*")>1), $C$3:$C$1000, "")), $C3)






