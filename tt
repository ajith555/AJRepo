# Retrieve the HTML code of the web page
$url = "https://www.example.com"
$html = Invoke-WebRequest -Uri $url

# Extract the table data from the HTML
$table = $html.ParsedHtml.getElementsByTagName("table") | Select-Object -First 1
if ($table -ne $null) {
    $rows = $table.getElementsByTagName("tr") | Select-Object -Skip 1
    if ($rows.Count -gt 0) {
        $data = foreach ($row in $rows) {
            $cells = $row.getElementsByTagName("td")
            if ($cells.Count -gt 0) {
                [pscustomobject]@{
                    Column1 = $cells[0].innerText
                    Column2 = $cells[1].innerText
                    Column3 = $cells[2].innerText
                }
            }
        }

        # Save the data in an Excel file
        $excel = New-Object -ComObject Excel.Application
        $workbook = $excel.Workbooks.Add()
        $worksheet = $workbook.Worksheets.Add()

        $row = 1
        foreach ($item in $data) {
            $worksheet.Cells.Item($row, 1) = $item.Column1
            $worksheet.Cells.Item($row, 2) = $item.Column2
            $worksheet.Cells.Item($row, 3) = $item.Column3
            $row++
        }

        $workbook.SaveAs("C:\path\to\file.xlsx")
        $workbook.Close()
        $excel.Quit()
    }
    else {
       
