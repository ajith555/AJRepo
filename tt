# Install the ImportExcel module
Install-Module -Name ImportExcel

# Set the URL of the web page
$url = "https://example.com/table.html"

# Use Invoke-WebRequest to retrieve the HTML of the web page
$response = Invoke-WebRequest -Uri $url

# Use Import-HTML to extract the table data and save it to an Excel sheet
$table = $response.ParsedHtml.getElementsByTagName("table") | Select-Object -First 1
$data = $table | ConvertTo-Html -Fragment | Out-String | Import-Html -WorksheetName "Table Data" -HeaderRow 1
$data | Export-Excel -Path "C:\data.xlsx"
