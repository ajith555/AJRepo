# Define the URL of the HTML page
$url = "https://example.com/page.html"

# Download the HTML content using Invoke-WebRequest
$html = Invoke-WebRequest -Uri $url

# Define the XPath query to select the data you want
$xpath = "//div[@class='my-class']//h2"

# Use Select-Xml to apply the XPath query to the HTML content
$data = $html.Content | Select-Xml -XPath $xpath | Select-Object -ExpandProperty Node | Select-Object -ExpandProperty InnerText

# Output the data
$data
